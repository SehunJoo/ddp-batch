#!/bin/bash

# check before proceeding


echo "Installing franks ..."

echo && ls -l --color  $DDP

if [[ -f $DDP/franks.orig ]]; then
    echo "franks is already installed, update franks"
    cp ./franks $DDP/franks
else
    mv $DDP/franks $DDP/franks.orig
    cp ./franks $DDP/franks
fi


echo && ls -l --color  $DDP

exit 0
